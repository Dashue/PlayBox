{
  "name": "oppdw",
  "version": "1.0.0",
  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4200",
    "start-mobile-testing": "ng serve --host 0.0.0.0 --disable-host-check",
    "start-dev-server": "ng serve --target=production --env=dev-server --port 4200",
    "start-test": "ng serve --target=production --env=test --port 4200",
    "start-prod": "ng serve --target=production --env=prod --port 4200",
    "build": "ng build --target=development --env=dev -bh /oppdw_mobile/",
    "build-dev-server": "ng build --target=production --env=dev-server -bh /oppdw_mobile/",
    "build-test": "ng build --target=production --env=test -bh /oppdw_mobile/",
    "build-prod": "ng build --target=production --env=prod -bh /oppdw_mobile/",
    "clean-deployment": "rm -rf oppdw_mobile/",
    "deploy": "npm run clean-deployment; cp -R ./dist/ oppdw_mobile/",
    "deploy-prod": "npm run clean-deployment; cp -R ./dist/ oppdw_mobile/",
    "test": "ng test --sourcemaps=false",
    "lint": "ng lint --type-check",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
